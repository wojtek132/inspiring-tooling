{
  "name": "tweet-fetcher-front",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "jsx": "react",
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "target"
  ],
  "files": [
    "app/front/main-entry.tsx"
  ]
}